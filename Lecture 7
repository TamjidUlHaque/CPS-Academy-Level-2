void IcpcDhakaRegionalist2025(){
    ll n , q ; cin >> n >> q ; vll vec(n) ; tamjid(n) cin >> vec[i] ;
    vll preCalc(n+1,0) ; tamjid(n) preCalc[i+1] ^= (preCalc[i] ^ vec[i]) ;
    while(q--){
        ll l , r ; cin >> l >> r ;
        cout << (preCalc[r] ^ preCalc[l-1]) << sesh ;
