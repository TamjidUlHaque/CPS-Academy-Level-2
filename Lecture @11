______________________________________https://codeforces.com/group/isP4JMZTix/contest/379072/problem/N______________________________________
                                                    Binary Search Approach
                                          -----------------------------------------------
void IcpcDhakaRegionalist2025(){
    ll n ; cin >> n ; bool isPossible = false ;
    for(ll i = 1 ; i * i * i <= n ; i++){
        ll y = n - i*i*i ;
        if(y<=0) break ;
        ll low = 1 ; ll high = 1e4 ; 
        while(low<high){
            ll mid = low + (high-low)/2 ;
            if(mid*mid*mid<y) low = mid+1 ;
            else high = mid ;
        }
        if(low*low*low==y){
            isPossible = true ; break ;
        }
    }
    if(isPossible) yes ;
    else no ;
}
                                                      PreCalculation Method
                                        -------------------------------------------------------
void IcpcDhakaRegionalist2025(){
    ll n ; cin >> n ; unordered_set<ll> storeCubes ;
    for(ll i = 1 ; i <= 1e4 ; i++){
        storeCubes.insert(i*i*i) ;
    }
    for(ll i = 1 ; i <= 1e4 ; i++){
        if(storeCubes.count(n-(i*i*i))){
            yes ; return ;
        } 
    }
    no ;
}
