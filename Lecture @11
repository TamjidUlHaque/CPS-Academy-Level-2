void IcpcDhakaRegionalist2025(){
    ll n ; cin >> n ; vll vec(n) ; tamjid(n) cin >> vec[i] ;
    nsort(vec) ; ll query ; cin >> query ; 
    while(query-->0){
        ll coin ; cin >> coin ; 
        ll ans = upper_bound(all(vec),coin) - vec.begin() ;
        cout << ans << sesh ;
    }
}
______________________________________https://codeforces.com/group/isP4JMZTix/contest/379072/problem/G______________________________________

ll n , k ; 
bool isOkay(ll y){
    ll x = y - y/n ;
    return x >= k ;
}
void IcpcDhakaRegionalist2025(){
    cin >> n >> k ;
    ll low = 1 ; ll high = 1e18 ; 
    while(low<high){
        ll mid = low + (high-low)/2 ;
        if(isOkay(mid)) high = mid ;
        else low = mid+1 ;
    }
    cout << high << endl ;
}


